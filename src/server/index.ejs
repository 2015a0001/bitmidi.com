<!doctype html>
<html lang='en'>
  <head>
    <title><%= typeof title !== 'undefined' ? title : config.name %></title>
    <meta charset='utf-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1' />

    <link rel='preload' href='/bundle.js?c=<%- bundleHash %>' as='script' />

    <%- typeof styleTag !== 'undefined' ? styleTag : '' %>

    <link rel='apple-touch-icon' sizes='180x180' href='/apple-touch-icon.png' />
    <link rel='icon' type='image/png' sizes='32x32' href='/favicon-32x32.png' />
    <link rel='icon' type='image/png' sizes='16x16' href='/favicon-16x16.png' />
    <link rel='manifest' href='/manifest.json' />
    <link rel='mask-icon' href='/safari-pinned-tab.svg' color='#ff41b4' />
    <meta name='apple-mobile-web-app-title' content='Node Foo' />
    <meta name='application-name' content='Node Foo' />
    <meta name='theme-color' content='#ff41b4' />
    <meta name='apple-mobile-web-app-capable' content='yes' />

    <!-- <link rel='search' href='/opensearch.xml' title='<%= config.name %>' type='application/opensearchdescription+xml'> -->
  </head>

  <body class='sans-serif f5 fw3 ma0 pa0 overflow-y-scroll bg-black-025'>
    <%- typeof content !== 'undefined' ? content : '' %>

    <script>
      console.time('render')
      window.storeInit =
        <%- typeof store !== 'undefined'
          ? JSON.stringify(store).replace(/<\//ig, '<\\/') // Prevent XSS
          : '{}'
        %>
    </script>
    <script>
      window.GoogleAnalyticsObject = 'ga'
      window.ga = window.ga || function () {
        (window.ga.q = window.ga.q || []).push(arguments)
      }
      window.ga.l = Date.now()
      window.ga('create', 'UA-3898076-23', 'auto')
    </script>

    <script async src='/bundle.js?c=<%- bundleHash %>'></script>
    <script async src='https://www.google-analytics.com/analytics.js'></script>
  </body>
</html>
