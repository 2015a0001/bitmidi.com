{
  "name": "nodefoo.com",
  "description": "Node.js examples and code snippets",
  "version": "1.0.0",
  "author": {
    "name": "Feross Aboukhadijeh",
    "email": "feross@feross.org",
    "url": "http://feross.org"
  },
  "babel": {
    "plugins": [
      "transform-class-properties",
      [
        "transform-react-jsx",
        {
          "pragma": "jsx",
          "useBuiltIns": true
        }
      ],
      "transform-object-rest-spread"
    ]
  },
  "browser": {
    "./src/api/index.js": "./src/browser/api.js",
    "./secret/index.js": false,
    "url": false
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "bugs": {
    "url": "https://github.com/feross/nodefoo.com/issues"
  },
  "dependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-register": "^6.26.0",
    "classnames": "^2.2.5",
    "clipboard-copy": "^1.2.0",
    "codemirror": "^5.31.0",
    "compression": "^1.6.2",
    "connect-sqlite3": "^0.9.9",
    "debug": "^3.0.0",
    "downgrade": "^1.1.1",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "highlight.js": "^9.12.0",
    "hyphenate-style-name": "^1.0.2",
    "login-with-twitter": "^1.2.0",
    "markdown-it": "^8.3.2",
    "memo-async-lru": "^1.0.1",
    "mollusc": "^1.2.0",
    "opbeat": "^4.14.0",
    "path-to-regexp": "^2.1.0",
    "preact": "^8.2.1",
    "preact-context-provider": "^1.0.0",
    "run-parallel": "^1.1.6",
    "sqlite3": "^3.1.9",
    "tachyons": "^4.7.4",
    "throttleit": "^1.0.0",
    "twit": "^2.2.9",
    "undom": "^0.3.3",
    "unlimited": "^1.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-minify": "^0.2.0",
    "babelify": "^7.3.0",
    "browser-pack-flat": "^3.0.1",
    "browserify": "^14.5.0",
    "browserify-breakdown": "^1.0.3",
    "common-shakeify": "^0.4.4",
    "git-pull-or-clone": "^1.1.0",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.11.0",
    "path-exists": "^3.0.0",
    "rimraf": "^2.6.1",
    "standard": "*",
    "tape": "^4.8.0",
    "watchify": "^3.9.0"
  },
  "homepage": "https://nodefoo.com",
  "license": "-",
  "main": "src/server/index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/feross/nodefoo.com.git"
  },
  "scripts": {
    "build": "npm run build-js && chmod 777 db/",
    "build-js": "browserify -i preact/devtools -p common-shakeify -p browser-pack-flat/plugin src/browser | minify > static/bundle.js",
    "build-js-debug": "browserify -d --full-paths src/browser > static/bundle.js",
    "generate": "DEBUG=* node ./bin/generate.js",
    "secret-download": "rsync -a -O -v --delete -e \"ssh -p 44444\" feross@future:\"/home/feross/www/nodefoo.com/secret/\" secret/",
    "secret-upload": "rsync -a -O -v --delete -e \"ssh -p 44444\" secret/ feross@future:\"/home/feross/www/nodefoo.com/secret/\"",
    "size": "npm run build-js && cat static/bundle.js | gzip | wc -c",
    "size-breakdown": "npm run build-js-debug && cat static/bundle.js | node ./node_modules/.bin/browserify-breakdown",
    "start": "node src/server",
    "test": "standard && tape test/*.js",
    "watch": "npm run watch-js & npm run watch-server",
    "watch-js": "watchify src/browser -d -v -o static/bundle.js",
    "watch-server": "DEBUG=nodefoo* DEBUG_DEPTH=1 nodemon src/server -e js,css,ejs -i static/ -i src/views"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
